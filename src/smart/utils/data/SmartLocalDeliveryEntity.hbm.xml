<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="smart.utils.data.SmartLocalDeliveryEntity" table="smart_local_delivery" schema="smartmall">
        <id name="deliverid" column="deliverid"/>
        <property name="orderid" column="orderid"/>
        <property name="uid" column="uid"/>
        <property name="type" column="type"/>
        <property name="status" column="status"/>
        <property name="sender" column="sender"/>
        <property name="address" column="address"/>
        <property name="carrier" column="carrier"/>
        <property name="starttime" column="starttime"/>
        <property name="reservetime" column="reservetime"/>
        <property name="packagetime" column="packagetime"/>
        <property name="accepttime" column="accepttime"/>
        <property name="confirmtime" column="confirmtime"/>
        <property name="logs" column="logs"/>
    </class>
</hibernate-mapping>