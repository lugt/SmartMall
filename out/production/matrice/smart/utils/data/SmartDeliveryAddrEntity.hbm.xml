<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="smart.utils.data.SmartDeliveryAddrEntity" table="smart_delivery_addr" schema="smartmall">
        <id name="addrid" column="addrid"/>
        <property name="uid" column="uid"/>
        <property name="mobile" column="mobile"/>
        <property name="recver" column="recver"/>
        <property name="type" column="type"/>
        <property name="province" column="province"/>
        <property name="city" column="city"/>
        <property name="district" column="district"/>
        <property name="addr" column="addr"/>
        <property name="memo" column="memo"/>
    </class>
</hibernate-mapping>