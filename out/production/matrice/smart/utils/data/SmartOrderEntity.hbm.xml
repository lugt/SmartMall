<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="smart.utils.data.SmartOrderEntity" table="smart_order" schema="smartmall">
        <id name="id" column="id"/>
        <property name="userId" column="user_id"/>
        <property name="paymentId" column="payment_id"/>
        <property name="deliveryId" column="delivery_id"/>
        <property name="merchandise" column="merchandise"/>
        <property name="status" column="status"/>
        <property name="payStatus" column="pay_status"/>
        <property name="distributionStatus" column="distribution_status"/>
        <property name="dueAmount" column="due_amount"/>
        <property name="paidAmount" column="paid_amount"/>
        <property name="taxes" column="taxes"/>
        <property name="payableFreight" column="payable_freight"/>
        <property name="realFreight" column="real_freight"/>
        <property name="payFee" column="pay_fee"/>
        <property name="promotions" column="promotions"/>
        <property name="discount" column="discount"/>
        <property name="orderAmount" column="order_amount"/>
        <property name="payTime" column="pay_time"/>
        <property name="sendTime" column="send_time"/>
        <property name="createTime" column="create_time"/>
        <property name="completionTime" column="completion_time"/>
        <property name="acceptTime" column="accept_time"/>
        <property name="invoice" column="invoice"/>
        <property name="invoiceTitle" column="invoice_title"/>
        <property name="postscript" column="postscript"/>
        <property name="note" column="note"/>
        <property name="prop" column="prop"/>
        <property name="exp" column="exp"/>
        <property name="point" column="point"/>
        <property name="type" column="type"/>
    </class>
</hibernate-mapping>